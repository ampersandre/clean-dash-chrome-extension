{
    "name": "Clean Dashboard",
    "version": "1.2",
    "description": "Remove the extra UI from dashboards. Icon made by https://www.flaticon.com/authors/smashicons",
    "manifest_version": 2,
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "menu.html",
      "default_icon": {
        "16": "images/dashboard16.png",
        "32": "images/dashboard32.png",
        "48": "images/dashboard48.png",
        "128": "images/dashboard128.png"
      }
    },
    "icons": {
      "16": "images/dashboard16.png",
      "32": "images/dashboard32.png",
      "48": "images/dashboard48.png",
      "128": "images/dashboard128.png"
    }
  }
