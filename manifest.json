{
    "name": "Clean Kibana Dash",
    "version": "1.0",
    "description": "Remove the extra UI from Kibana's dashboard",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent"],
    "background": {
      "scripts": ["detect-kibana.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "menu.html",
      "default_icon": {
        "16": "images/kibana16.png",
        "32": "images/kibana32.png",
        "48": "images/kibana48.png",
        "128": "images/kibana128.png"
      }
    },
    "icons": {
      "16": "images/kibana16.png",
      "32": "images/kibana32.png",
      "48": "images/kibana48.png",
      "128": "images/kibana128.png"
    }
  }